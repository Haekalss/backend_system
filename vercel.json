{
  "version": 2,
  "buildCommand": "npm install",
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "PORT": "3001"
  },
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    }
  ]
}
